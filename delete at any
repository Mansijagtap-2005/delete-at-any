#include <stdio.h>
//at specific

int main() {
  int i,arr[10],n,pos;
  printf("enter size of array:");
  scanf("%d",&n);
  printf("Enter the elements of the array: ");
  for (int i = 0; i < n; i++) {
    scanf("%d", &arr[i]);
  }
printf("enter position:");

scanf("%d",&pos);
for(int i=pos-1;i<n-1;i++){
  arr[i]=arr[i+1];
}
for(i=0;i<n-1;i++){
    printf("%d",arr[i]);
}
return 0;
}
